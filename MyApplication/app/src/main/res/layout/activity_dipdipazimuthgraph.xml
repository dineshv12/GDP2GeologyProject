<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_dipdipazimuthgraph"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.s525127.myapplication.dipdipazimuthgraph">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView11"
        android:layout_below="@+id/dipdipazimuth"
        android:layout_marginTop="27dp"
        android:layout_alignEnd="@+id/dipdipazimuth"
        android:textSize="20sp" />

    <ImageView
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/strikedip"
        android:id="@+id/imageView"
        android:layout_below="@+id/dipdipazimuth"
        android:layout_alignParentStart="true"
        android:layout_width="200dip" />

    <com.jjoe64.graphview.GraphView
        android:id="@+id/dipdipazimuth"
        android:layout_height="350dip"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="33dp"
        android:layout_width="400dip" />

    <TextView
        android:text="Dip/Dip Azimuth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:id="@+id/textView12"
        android:layout_above="@+id/imageView2"
        android:textSize="20sp"
        android:layout_toStartOf="@+id/imageView2" />

    <ImageView
        android:layout_height="100dip"
        app:srcCompat="@drawable/direction"
        android:id="@+id/imageView2"
        android:layout_alignTop="@+id/dipdipazimuth"
        android:layout_alignEnd="@+id/dipdipazimuth"
        android:layout_width="100dip" />
</RelativeLayout>
