<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.s525127.myapplication.strikedippage">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Select Problem Type"
        android:id="@+id/textView3"
        android:layout_marginStart="30dp"
        android:layout_marginTop="32dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />



    <Button
        android:text="Quadrant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button7"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/textView3"
        android:layout_alignEnd="@+id/button8"
        android:layout_alignStart="@+id/button8" />

    <Button
        android:text="Azimuth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button7"
        android:layout_marginTop="56dp"
        android:id="@+id/button8"
        android:layout_alignEnd="@+id/button9"
        android:layout_alignStart="@+id/button9" />

    <Button
        android:text="Dip/Dip Azimuth"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button9"
        android:layout_alignStart="@+id/button9"
        android:layout_marginTop="64dp"
        android:id="@+id/button10"
        android:layout_alignEnd="@+id/button9" />

    <Button
        android:text=" Dip/Dip Quadrant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="49dp"
        android:id="@+id/button9"
        android:layout_below="@+id/button8"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
